                    <my:SimpleStackPanel Orientation="Horizontal">
                        <smtx:XamlDisplay UniqueKey="gb_0_1_0">
                            <GroupBox Style="{StaticResource MyNet.Styles.GroupBox.Elevation.Header}" my:IconAssist.Icon="{my:Icon Kind=Soccer}" Header="Header">
