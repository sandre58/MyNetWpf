                        </smtx:XamlDisplay>
                        <smtx:XamlDisplay UniqueKey="gb_0_0_3">
                            <GroupBox Style="{StaticResource MyNet.Styles.GroupBox.Elevation.Accent}" my:IconAssist.Icon="{my:Icon Kind=Soccer}" Header="Header">
