                        <smtx:XamlDisplay UniqueKey="gb_3_0_3">
                            <GroupBox Style="{StaticResource MyNet.Styles.GroupBox.Clean.Bottom}" Header="Header">
                                <TextBox Style="{StaticResource MyNet.Styles.TextBox.Filled}" Width="100" />
                            </GroupBox>
                        </smtx:XamlDisplay>
                    </my:SimpleStackPanel>

                    <my:SimpleStackPanel>
                        <smtx:XamlDisplay UniqueKey="gb_3_1_0">
                            <GroupBox Style="{StaticResource MyNet.Styles.GroupBox.Clean.Caption}" my:IconAssist.Icon="{my:Icon Kind=Soccer}" Header="Header">
