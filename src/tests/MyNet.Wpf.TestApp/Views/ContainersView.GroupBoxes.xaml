                                    <Button Content="{my:Icon Kind=Settings}" />
                                </my:GroupBoxAssist.MoreContent>
                                <contents:SmallContent />
                            </GroupBox>
                        </smtx:XamlDisplay>
                    </my:SimpleStackPanel>
                </my:SimpleStackPanel>
            </Expander>


            <Expander Style="{StaticResource MyTestApp.Styles.Expander}" Header="MyNet.Styles.GroupBox.Bordered">
                <my:SimpleStackPanel p:EnabledAssist.CanBeDisabled="True" Orientation="Horizontal">
                    <my:SimpleStackPanel Orientation="Horizontal">
                        <smtx:XamlDisplay UniqueKey="gb_2_0_0">
                            <GroupBox Style="{StaticResource MyNet.Styles.GroupBox.Bordered}" my:IconAssist.Icon="{my:Icon Kind=Soccer}" Header="Header">
                                <my:GroupBoxAssist.MoreContent>
                                    <Button Content="{my:Icon Kind=Settings}" />
                                </my:GroupBoxAssist.MoreContent>
                                <contents:SmallContent />
                            </GroupBox>
                        </smtx:XamlDisplay>
                        <smtx:XamlDisplay UniqueKey="gb_2_0_1">
                            <GroupBox Style="{StaticResource MyNet.Styles.GroupBox.Bordered.Application}" my:IconAssist.Icon="{my:Icon Kind=Soccer}" Header="Header">
                                <my:GroupBoxAssist.MoreContent>
