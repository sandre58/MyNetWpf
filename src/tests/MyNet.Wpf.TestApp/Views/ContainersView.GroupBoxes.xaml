            <Expander Style="{StaticResource MyTestApp.Styles.Expander}" Header="MyNet.Styles.GroupBox.Caption.Clean">
                <my:SimpleStackPanel p:EnabledAssist.CanBeDisabled="True">

                    <my:SimpleStackPanel Orientation="Horizontal">
                        <smtx:XamlDisplay UniqueKey="gb_3_0_0">
                            <GroupBox Style="{StaticResource MyNet.Styles.GroupBox.Clean}" Header="Header">
                                <TextBox Style="{StaticResource MyNet.Styles.TextBox.Filled}" Width="100" />
                            </GroupBox>
                        </smtx:XamlDisplay>

                        <smtx:XamlDisplay UniqueKey="gb_3_0_1">
                            <GroupBox Style="{StaticResource MyNet.Styles.GroupBox.Clean.Left}" Header="{my:MediumIcon Kind=Soccer}">
                                <TextBox Style="{StaticResource MyNet.Styles.TextBox.Filled}" Width="100" />
                            </GroupBox>
                        </smtx:XamlDisplay>

                        <smtx:XamlDisplay UniqueKey="gb_3_0_2">
                            <GroupBox Style="{StaticResource MyNet.Styles.GroupBox.Clean.Right}" Header="{my:MediumIcon Kind=Soccer}">
