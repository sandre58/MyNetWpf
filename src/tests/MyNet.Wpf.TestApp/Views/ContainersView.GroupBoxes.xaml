                    <my:SimpleStackPanel Orientation="Horizontal">
                        <smtx:XamlDisplay UniqueKey="gb_0_0_0">
                            <my:Badged Badge="{my:Resource Default}">
                                <GroupBox Style="{StaticResource MyNet.Styles.GroupBox.Elevation}" my:IconAssist.Icon="{my:Icon Kind=Soccer}" Header="Header">
                                    <my:GroupBoxAssist.MoreContent>
                                        <Button Content="{my:Icon Kind=Settings}" />
                                    </my:GroupBoxAssist.MoreContent>
                                    <contents:SmallContent />
                                </GroupBox>
                            </my:Badged>
                        </smtx:XamlDisplay>
                        <smtx:XamlDisplay UniqueKey="gb_0_0_1">
                            <GroupBox Style="{StaticResource MyNet.Styles.GroupBox.Elevation.Application}" my:IconAssist.Icon="{my:Icon Kind=Soccer}" Header="Header">
                                <my:GroupBoxAssist.MoreContent>
                                    <Button Content="{my:Icon Kind=Settings}" />
