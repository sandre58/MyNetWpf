                            </GroupBox>
                        </smtx:XamlDisplay>
                        <smtx:XamlDisplay UniqueKey="gb_0_1_3">
                            <GroupBox Style="{StaticResource MyNet.Styles.GroupBox.Elevation.Header.Accent}" my:IconAssist.Icon="{my:Icon Kind=Soccer}" Header="Header">
                                <my:GroupBoxAssist.MoreContent>
                                    <Button Content="{my:Icon Kind=Settings}" />
                                </my:GroupBoxAssist.MoreContent>
