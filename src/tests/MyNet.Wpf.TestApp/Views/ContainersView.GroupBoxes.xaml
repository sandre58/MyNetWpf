                        </smtx:XamlDisplay>
                        <smtx:XamlDisplay UniqueKey="gb_0_0_4">
                            <GroupBox Style="{StaticResource MyNet.Styles.GroupBox.Elevation.Foreground}" my:IconAssist.Icon="{my:Icon Kind=Soccer}" Header="Header">
                                <my:GroupBoxAssist.MoreContent>
                                    <Button Content="{my:Icon Kind=Settings}" />
                                </my:GroupBoxAssist.MoreContent>
                                <contents:SmallContent />
                            </GroupBox>
                        </smtx:XamlDisplay>
                        <smtx:XamlDisplay UniqueKey="gb_0_0_5">
                            <GroupBox Style="{StaticResource MyNet.Styles.GroupBox.Elevation.Custom}"
                                      my:IconAssist.Icon="{my:Icon Kind=Soccer}"
