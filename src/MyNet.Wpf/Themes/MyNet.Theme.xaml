    <Style x:Key="{x:Static ToolBar.TextBoxStyleKey}" BasedOn="{StaticResource MyNet.Styles.TextBox.ToolBar}" TargetType="{x:Type TextBox}" />
    <Style x:Key="{x:Static ToolBar.ComboBoxStyleKey}" BasedOn="{StaticResource MyNet.Styles.ComboBox.ToolBar}" TargetType="{x:Type ComboBox}" />
</ResourceDictionary>