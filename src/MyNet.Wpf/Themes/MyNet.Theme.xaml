<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:wpf="clr-namespace:MyNet.Wpf">
    <ResourceDictionary.MergedDictionaries>
        <wpf:SharedResourceDictionary Source="pack://application:,,,/MyNet.Wpf;component/Themes/MyNet.Styles.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <!--  Default  -->
    <Style BasedOn="{StaticResource MyNet.Styles.Button.Filled}" TargetType="{x:Type Button}" />
    <Style BasedOn="{StaticResource MyNet.Styles.Calendar.Portrait}" TargetType="{x:Type Calendar}" />
    <Style BasedOn="{StaticResource MyNet.Styles.CheckBox}" TargetType="{x:Type CheckBox}" />
    <Style BasedOn="{StaticResource MyNet.Styles.ComboBox}" TargetType="{x:Type ComboBox}" />
    <Style BasedOn="{StaticResource MyNet.Styles.ComboBoxItem}" TargetType="{x:Type ComboBoxItem}" />
    <Style BasedOn="{StaticResource MyNet.Styles.ContentControl}" TargetType="{x:Type ContentControl}" />
    <Style BasedOn="{StaticResource MyNet.Styles.ContextMenu}" TargetType="{x:Type ContextMenu}" />
    <Style BasedOn="{StaticResource MyNet.Styles.DataGrid}" TargetType="{x:Type DataGrid}" />
    <Style BasedOn="{StaticResource MyNet.Styles.DataGridCell}" TargetType="{x:Type DataGridCell}" />
    <Style BasedOn="{StaticResource MyNet.Styles.DataGridColumnHeader}" TargetType="{x:Type DataGridColumnHeader}" />
    <Style BasedOn="{StaticResource MyNet.Styles.DataGridRow}" TargetType="{x:Type DataGridRow}" />
