    <Style x:Key="{x:Static MenuItem.SeparatorStyleKey}" BasedOn="{StaticResource MyNet.Styles.Separator.Horizontal}" TargetType="{x:Type Separator}" />

    <Style x:Key="{x:Static SystemParameters.FocusVisualStyleKey}" BasedOn="{StaticResource MyNet.Styles.FocusVisualStyle.Rectangle}" />

    <Style x:Key="{x:Static ToolBar.SeparatorStyleKey}" BasedOn="{StaticResource MyNet.Styles.Separator.Vertical}" TargetType="{x:Type Separator}" />
